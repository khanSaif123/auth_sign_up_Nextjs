{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/app/**/*.tsx",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app/$1"
    }
  ]
}


  